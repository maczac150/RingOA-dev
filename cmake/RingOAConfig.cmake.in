@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Allow THIRD_PARTY_DIR to be passed in (optional hint)
if(NOT DEFINED THIRD_PARTY_DIR AND DEFINED ENV{THIRD_PARTY_DIR})
  set(THIRD_PARTY_DIR "$ENV{THIRD_PARTY_DIR}")
endif()

# Resolve required dependency: cryptoTools (CONFIG mode)
if(DEFINED THIRD_PARTY_DIR)
  find_dependency(cryptoTools CONFIG REQUIRED HINTS "${THIRD_PARTY_DIR}/unix")
else()
  find_dependency(cryptoTools CONFIG REQUIRED)
endif()

# Import RingOA exported targets
include("${CMAKE_CURRENT_LIST_DIR}/RingOATargets.cmake")

# Convenience variable
set(RingOA_VERSION "@PROJECT_VERSION@")
